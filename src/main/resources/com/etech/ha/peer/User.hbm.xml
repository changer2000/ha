<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="com.etech.ha.peer">
	<class name="UserPeer" table="t_user">
		<id name="id" column="id" >
			<generator class="native"></generator>
		</id>
		<property name="empe_num" column="empe_num" type="string"></property>
		<property name="empe_name" column="empe_name" type="string"></property>
		<property name="pwd" column="pwd" type="string"></property>
		<property name="mobile" column="mobile" type="string"></property>
		<property name="tel_no" column="tel_no" type="string"></property>
		<property name="email" column="email" type="string"></property>
		<property name="dept_cd" column="dept_cd" type="string"></property>
		<property name="del_flg" column="del_flg" type="int"></property>
		
	</class>
</hibernate-mapping>